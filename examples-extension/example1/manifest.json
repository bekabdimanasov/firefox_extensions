{

  "manifest_version": 2,
  "name": "Example1",
  "version": "1.0",
  "description": "Firefox extension example first",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "permissions": [
    "private",
   "proxy",
    "tabs",
    "history"
  ]
}
